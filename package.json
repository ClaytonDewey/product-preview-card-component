{
  "name": "product-preview-card-component",
  "version": "1.0.0",
  "description": "challenge for frontendmentor.io",
  "main": "index.js",
  "scripts": {
    "watch:sass": "node-sass sass/main.scss css/style.3.1.0.css -w --souce-map true",
    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver watch:sass",
    "compile:sass": "node-sass sass/main.scss css/style.comp.css",
    "concat:css": "concat -o css/style.concat.css css/style.comp.css",
    "prefix:css": "postcss --use autoprefixer -b \"last 10 versions\" css/style.concat.css -o css/style.prefix.css",
    "compress:css": "node-sass css/style.prefix.css css/style.min.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ClaytonDewey/product-preview-card-component.git"
  },
  "author": "Clayton Dewey",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ClaytonDewey/product-preview-card-component/issues"
  },
  "homepage": "https://github.com/ClaytonDewey/product-preview-card-component#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.8",
    "concat": "^1.0.3",
    "eslint": "^8.20.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-jquery": "^3.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "parcel-bundler": "^1.12.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^10.0.0",
    "prettier": "^2.7.1"
  },
  "dependencies": {
    "live-server": "^1.2.2"
  }
}
